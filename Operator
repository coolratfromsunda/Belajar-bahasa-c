#include <stdio.h>

int main() {
    int a = 9, b = 4, c;

    c = a + b;
    printf("a + b = \n%d", c);

    c = a - b;
    printf("a + b = \n%d", c);

    c = a * b;
    printf("a + b = \n%d", c);

    c = a / b;
    printf("a + b = \n%d", c);

    c = a % b;
    printf("a + b = \n%d", c);

return 0;
}
