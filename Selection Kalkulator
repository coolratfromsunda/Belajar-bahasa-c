#include <stdio.h>
#include <stdlib.h>

int main()
{
    double a,b;
    char op;

    scanf("%lf %c %lf", &a,&op,&b);
    if(op=='+') {
       printf("\n%2.lf %c %2.lf = %2.lf\n",a,op,b,a+b);
       }
       else if(op=='-') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a-b);
       }
       else if(op=='*') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a*b);
       }
       else if(op=='/') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a/b);
       }


     scanf("\n%lf %c %lf", &a,&op,&b);
     if(op=='+') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a+b);
       }
       else if(op=='-') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a-b);
       }
       else if(op=='*') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a*b);
       }
       else if(op=='/') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a/b);
       }


      scanf("\n%lf %c %lf", &a,&op,&b);
      if(op=='+') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a+b);
       }
       else if(op=='-') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a-b);
       }
       else if(op=='*') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a*b);
       }
       else if(op=='/') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a/b);
       }


       scanf("%lf %c %lf", &a,&op,&b);
    if(op=='+') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a+b);
       }
       else if(op=='-') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a-b);
       }
       else if(op=='*') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a*b);
       }
       else if(op=='/') {
       printf("%2.lf %c %2.lf = %2.lf\n",a,op,b,a/b);
       }
    return 0;
}
